        <encodingDesc>
            <tagsDecl>
                <pb:behaviour ident="article" output="web">
                    <pb:param name="id" select="@xml:id" />
                    <pb:template xmlns="" xml:space="preserve">
                        <article class="entry" id="[[id]]">[[content]]</article>
                        <hr style="entry-delim" />
                    </pb:template>
                </pb:behaviour>
                <pb:behaviour ident="entry-polysemy" output="web">
                    <pb:param name="id" />
                    <pb:param name="summary-lemma" />
                    <pb:param name="summary-sense-n" />
                    <pb:param name="summary-sense" />
                    <pb:param name="summary-frequency" />
                    <pb:param name="rest" />
                    <pb:template xmlns="" xml:space="preserve">
                        <article class="entry" id="[[id]]">
                            <details>
                                <summary>
                                    <h4>
                                        <input type="checkbox" />
                                        <span class="tei-lemma">[[summary-lemma]]</span>
                                        <span class="tei-sense-num summary">[[summary-sense-n]]</span>
                                        <span class="tei-sense summary">[[summary-sense]]</span>
                                        [[summary-frequency]]
                                    </h4>
                                </summary>
                                [[rest]]
                                <form class="lediir-comment" action="comment.html">
                                    <input type="hidden" name="id" value="[[id]]" />
                                    <button type="submit">
                                        <iron-icon icon="editor:insert-comment" />
                                    </button>
                                </form>
                            </details>
                        </article>
                    </pb:template>
                </pb:behaviour>
                <pb:behaviour ident="entry" output="web">
                    <pb:param name="id" />
                    <pb:param name="summary-lemma" />
                    <pb:param name="summary-sense" />
                    <pb:param name="summary-frequency" />
                    <pb:param name="rest" />
                    <pb:template xmlns="" xml:space="preserve">
                        <article class="entry" id="[[id]]">
                            <details>
                                <summary>
                                    <h4>
                                        <input type="checkbox" />
                                        <span class="tei-lemma">[[summary-lemma]]</span>
                                        <span class="tei-sense summary">[[summary-sense]]</span>
                                        [[summary-frequency]]
                                    </h4>
                                </summary>
                                [[rest]]
                                <form class="lediir-comment" action="comment.html">
                                    <input type="hidden" name="id" value="[[id]]" />
                                    <button type="submit">
                                        <iron-icon icon="editor:insert-comment" />
                                    </button>
                                </form>
                            </details>
                        </article>
                    </pb:template>
                </pb:behaviour>
                <pb:behaviour ident="sense" output="web">
                    <pb:param name="id" select="@xml:id" />
                    <pb:param name="n" select="@n" />
                    <pb:template xmlns="" xml:space="preserve">
                        <div class="tei-sense" id="[[id]]">
                            <span class="tei-sense-num">[[n]]</span>
                            [[content]]
                        </div>
                    </pb:template>
                </pb:behaviour>
                <pb:behaviour ident="quote" output="web">
                    <pb:param name="lang" select="if(@xml:lang) then @xml:lang else ancestor::*[@xml:lang][1]/@xml:lang" />
                    <pb:template xmlns="" xml:space="preserve">
                        <span class="tei-quote" xml:lang="[[lang]]" lang="[[lang]]">[[content]]</span>
                        <span class="quote-index">☚</span>
                    </pb:template>
                </pb:behaviour>
                <pb:behaviour ident="translation" output="web">
                    <pb:param name="lang" select="if(@xml:lang) then @xml:lang else ancestor::*[@xml:lang][1]/@xml:lang" />
                    <pb:template xmlns="" xml:space="preserve">
                        <span class="translation-index">☛</span>
                        <span class="tei-translation" xml:lang="[[lang]]" lang="[[lang]]">[[content]]</span>
                    </pb:template>
                </pb:behaviour>
                <pb:behaviour ident="details" output="web">
                    <pb:param name="summary" />
                    <pb:template xmlns="" xml:space="preserve">
                        <details>
                            <summary>
                                <h4>[[summary]]</h4>
                            </summary>
                            [[content]]
                        </details>
                    </pb:template>
                </pb:behaviour>
                <pb:behaviour ident="pos" output="web">
                    <pb:param name="id" as="xs:string" />
                    <pb:param name="key-prefix" as="xs:string" />
                    <pb:template xmlns="" xml:space="preserve">
                        <pb-popover theme="translucent">
                            <span slot="default">
                                <pb-i18n key="[[key-prefix]].abbr">[[content]]</pb-i18n>
                            </span>
                            <pb:template slot="alternate">
                                <p>
                                    <pb-i18n key="[[key-prefix]].expand">[[content]]</pb-i18n>
                                </p>
                                <p>
                                    <pb-i18n key="[[key-prefix]].tooltip">[[content]]</pb-i18n>
                                </p>
                            </pb:template>
                        </pb-popover>
                        <span xml:space="preserve" class="space"> </span>
                    </pb:template>
                </pb:behaviour>
                <rendition source="lediir.css" />
            </tagsDecl>
        </encodingDesc>
        <revisionDesc>
